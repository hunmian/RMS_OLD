<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cqupt.mis.rms.model.TeachersAwardsNew" table="TEACHERS_AWARDS_NEW">
	    <id name="id">
	    	<generator class="native"></generator>
	    </id>
        <many-to-one name="teachAchievementsNew" column="achievements_id" class="com.cqupt.mis.rms.model.TeachAchievementsNew"></many-to-one>
        <property name="awardId" column="award_id" type="java.lang.String"/>
		<property name="memberName" column="member_name" />
		<property name="orders" column="orders" type="int" not-null="true"/>
		<property name="remarks" column="remarks" />
	</class>
</hibernate-mapping>
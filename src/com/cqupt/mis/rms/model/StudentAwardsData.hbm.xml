<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.cqupt.mis.rms.model">
	<class name="StudentAwardsData" table="student_awards_Data">
		<composite-id>	
			<key-many-to-one name="awards" column="awards_id" class="StudentAwardsRecord"></key-many-to-one>
			<key-many-to-one name="field" column="field_id" class="StudentAwardsField"></key-many-to-one> 
		</composite-id>
		
		<property name="value" column="value"></property> 
	</class>
</hibernate-mapping>
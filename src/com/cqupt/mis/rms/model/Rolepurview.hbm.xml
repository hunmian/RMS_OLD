<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
    <class name="com.cqupt.mis.rms.model.Rolepurview" table="t_rolepurview">
        <composite-id>
            <key-many-to-one name="roleinfo" class="com.cqupt.mis.rms.model.CQUPTRole">
                <column name="roleId" />
            </key-many-to-one>
            <key-many-to-one name="purviewinfo" class="com.cqupt.mis.rms.model.Purviewinfo">
                <column name="purviewId" />
            </key-many-to-one>
        </composite-id>
    </class>
</hibernate-mapping>

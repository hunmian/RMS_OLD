<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cqupt.mis.rms.model.TeachersAwardsField" table="teachers_awards_fields">
		<id name="id" column="field_id">
			<generator class="native"></generator>
		</id>
		<property name="name" column="field_name"></property>
		<property name="description" column="field_des"></property>
		<property name="order" column="`order`"></property>
		<property name="isDelete" column="is_delete"></property>
		<property name="submittime" column="submittime"></property>
		<set name="fieldDatas" table="teachers_awards_data" cascade="all" order-by="field_id">
			<key column="field_id"/>
			<one-to-many class="com.cqupt.mis.rms.model.TeachersAwardsData"/>
		</set>
	</class>
</hibernate-mapping>
<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cqupt.mis.rms.model.HumanitiesProject" table="HUMANITIES_PROJECT">
		<id name="projectId" column="project_id"/>
		<property name="projectName" column="project_name" not-null="true"></property>
		<property name="projectNumber" column="project_number"></property>
		<property name="projectOrigin" column="project_origin"></property>
		<property name="timeApproved" column="time_approved" type="java.sql.Date"></property>
		
		<many-to-one name="submitUser" class="com.cqupt.mis.rms.model.CQUPTUser" column="submit_user_id" lazy="false"></many-to-one>
		<many-to-one name="approvedUser" class="com.cqupt.mis.rms.model.CQUPTUser" column="approved_user_id" lazy="false"></many-to-one>
		
		<property name="status" column="status" not-null="true"/>
		<property name="returnReason" column="return_reason" />						
	</class>
</hibernate-mapping>